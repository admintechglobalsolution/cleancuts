
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(cuid())
  name      String
  contact   String     @unique
  status    UserStatus @default(New)

  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
}


enum UserStatus {
  New
  Active
}
